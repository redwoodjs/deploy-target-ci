{
  "environments": [
    {
      "id": "development",
      "name": "Development",
      "region": "us-east-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "redwood-api",
          "name": "Redwood API",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "port": 8911,
          "installCommand": "yarn set version berry && cd flightcontrol && NODE_ENV=development yarn install --immutable",
          "buildCommand": "yarn rw deploy flightcontrol api",
          "startCommand": "yarn rw deploy flightcontrol --serve",
          "postBuildCommand": "echo 0",
          "healthCheckPath": "/graphql/health"
        },
        {
          "id": "redwood-web",
          "name": "Redwood Web",
          "type": "static",
          "singlePageApp": true,
          "installCommand": "yarn set version berry && cd flightcontrol && NODE_ENV=development yarn install --immutable",
          "buildCommand": "yarn rw deploy flightcontrol web",
          "outputDirectory": "flightcontrol/web/dist"
        }
      ]
    }
  ]
}
